    "n = input()\n",
    "m = input()\n",
    "k_1 = []\n",
    "k_2 = []\n",
    "for i in range(int(n)):\n",
    "    k_1.append(input())\n",
    "# print(k_1)\n",
    "for i in range(int(m)):\n",
    "    k_2.append(input())\n",
    "# print(k_2)\n",
    "# if len(k_1) < len(k_2):\n",
    "#     k_1, k_2 = k_2, k_1\n",
    "s_intersection = set(k_1).symmetric_difference(set(k_2))\n",
    "# print(s_intersection)\n",
    "if len(s_intersection) == 0:\n",
    "    print('Таких нет')\n",
    "else:\n",
    "    print(len(s_intersection))"
   ]
