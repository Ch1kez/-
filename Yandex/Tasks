   "metadata": {},
   "outputs": [],
   "source": [
    "# ch = input()\n",
    "data = {}\n",
    "max_find = 0\n",
    "max_ch = ''\n",
    "while True:\n",
    "    ch = input().lower()\n",
    "    if ch == 'финиш':\n",
    "        break\n",
    "    for el in ch:\n",
    "        if el == ' ':\n",
    "            continue\n",
    "        if el in data:\n",
    "            data[el] += 1\n",
    "            if max_find < data[el]:\n",
    "                max_find = data[el]\n",
    "                max_ch = el\n",
    "            elif max_find == data[el]:\n",
    "                if el < max_ch:\n",
    "                    max_ch = el\n",
    "        else:\n",
    "            data[el] = 1\n",
    "print(max_ch)"
   ]
  },
